{
  "version": 2,

  "buildCommand": "npm install && npm run build:vercel",
  "outputDirectory": "dist/third-party-contracts-portal/browser",

  "framework": null,

  "buildCache": {
    "skipIntrospection": true
  },

  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=4096",
      "SKIP_BUILD_CACHE": "true"
    }
  },

  "rewrites": [
    {
      "source": "/rest/:path*",
      "destination": "http://protheusawsmobile.ddns.net:8080/rest/:path*"
    },
    {
      "source": "/((?!api|rest).*)",
      "destination": "/index.html"
    }
  ]
}
