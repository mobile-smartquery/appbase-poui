{
  "version": 2,

  "buildCommand": "npm run build",
  "outputDirectory": "dist/third-party-contracts-portal/browser",

  "rewrites": [
    {
      "source": "/rest/:path*",
      "destination": "/api/rest/:path*"
    },
    {
      "source": "/((?!api|rest).*)",
      "destination": "/index.html"
    }
  ]
}
