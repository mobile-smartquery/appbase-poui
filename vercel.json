{
  "version": 2,

  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist/third-party-contracts-portal/browser",

  "framework": null,

  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=4096"
    }
  },

  "rewrites": [
    {
      "source": "/rest/:path*",
      "destination": "http://protheusawsmobile.ddns.net:8080/rest/:path*"
    },
    {
      "source": "/((?!api|rest).*)",
      "destination": "/index.html"
    }
  ]
}
